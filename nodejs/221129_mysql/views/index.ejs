<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <table border style="text-align:center;" cellspacing="1" cellpadding="5" >
        <tr>
            <td>index</td>
            <td>id</td>
            <td>name</td>
        </tr>
        <%for (let i=0; i<rows.length ; i++) { %>
            <tr>
                <td><%=i+1%></td>
                <td><%=rows[i].id%></td>
                <td><%=rows[i].name%></td>
            </tr>
        <% } %>
    </table>
